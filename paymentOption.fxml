   <children>
      <Label fx:id="label" layoutX="188.0" layoutY="58.0" text="Payment Options">
         <font>
            <Font name="Brush Script MT Italic" size="40.0" />
         </font>
      </Label>
      <Button fx:id="bkash" layoutX="274.0" layoutY="175.0" mnemonicParsing="false" onAction="#on_click_btn_payWithBkash" text="Pay with Bkash" />
      <Button fx:id="cashOnDelivery" layoutX="274.0" layoutY="233.0" mnemonicParsing="false" onAction="#on_click_btn_cashOnDelivery" text="Cash on delivery" />
      <Label fx:id="labelCashOnDelivery" layoutX="308.0" layoutY="297.0" text="Label" />
   </children>
</AnchorPane>
