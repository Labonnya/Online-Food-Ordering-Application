   <children>
      <Label fx:id="title" layoutX="286.0" layoutY="71.0" text="Sorted Menu" />
      <Label fx:id="label" layoutX="286.0" layoutY="134.0" text="Label" />
      <Button fx:id="seeMenu" layoutX="274.0" layoutY="223.0" mnemonicParsing="false" onAction="#on_click_btn_seeMenu" text="See Menu" />
      <Button fx:id="order" layoutX="274.0" layoutY="296.0" mnemonicParsing="false" onAction="#switchToMenu" text="Order" />
   </children>
</AnchorPane>
