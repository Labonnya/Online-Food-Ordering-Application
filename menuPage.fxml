      <Label fx:id="menu" layoutX="233.0" layoutY="39.0" text="Menu">
         <font>
            <Font name="Brush Script MT Italic" size="59.0" />
         </font>
      </Label>
      <Button layoutX="219.0" layoutY="167.0" mnemonicParsing="false" onAction="#on_click_btn_duplicateMenuRemove" text="Click to see non Duplicate Menu" />
